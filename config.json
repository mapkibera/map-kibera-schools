{
  "static": {
    "js": {
      "prefix": "static/js",
      "files": [
        "libs/log-safety.js",
        "libs/smooth-scroll.js",
        "libs/utils.js",
        "libs/jquery.js",
        "libs/lodash.js",
        "libs/backbone.js",
        "libs/leaflet.js",
        "libs/fast-levenshtein.js",

        "sticky-nav.js",
        "slider.js",

        "app.js",
        "models/schools.js",
        "models/fields.js",
        "models/filters.js",
        "models/select-filter.js",
        "models/search-filter.js",
        "views/map-school-pin.js",
        "views/map-all-schools.js",
        "views/map-filters.js",
        "views/map-one-school.js",
        "views/map.js",
        "views/mapped-school.js",
        "views/schools-stats.js",
        "views/widget-select.js",
        "views/widget-quicksearch.js",
        "views/profile-data.js",
        "main.js"
      ],
      "filters": ["concat", "closure"]
    },
    "css": {
      "prefix": "static/css",
      "files": [
        "leaflet.css",

        "variables.scss",
        "icon-font.css",
        "mixins.scss",
        "normalize.css",
        "helpers.scss",
        "base.scss",

        "style.scss",
        "responsive.scss",
        "print.css"
      ],
      "filters": ["concat", "scss", "cssmin"]
    }
  },
  "build": {
    "staging": {
      "cname": "kibera-schools.dgstg.org",
      "root": "/",
      "robots": "static/root/robots-goaway.txt"
    },
    "production": {
      "cname": "openschoolskenya.org",
      "root": "/",
      "robots": "static/root/robots-welcome.txt"
    }
  }
}
